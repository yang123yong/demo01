<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sys.demo01.dao.GcmcDao">
    <select id="getGcmcById" resultType="Gcmc">
        SELECT gcmc_id,gcmc_name,khmc_id FROM gcmc where gcmc_id = #{gcmc_id}
    </select>
</mapper>